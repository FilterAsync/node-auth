<script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.9.2/dist/umd/popper.min.js" integrity="sha384-IQsoLXl5PILFhosVNubq5LC7Qb9DXgDA9i+tQ8Zj3iwWAwPtgFTxbJ8NT4GN1R8p" crossorigin="anonymous"></script>
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous" />
<link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.7.0/css/all.css" integrity="sha384-lZN37f5QGtY3VHgisS14W3ExzMWZxybE1SJSEsQp9S+oqd12jhcu+A56Ebc1zFSJ" crossorigin="anonymous" />
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>
<script type="text/javascript">
(function() {
    "use strict";
    document.getCookie = cookieName => {
        const value = `; ${document.cookie}`;
        const parts = value.split(`; ${cookieName}=`);
        if (parts.length === 2) {
            return parts.pop().split(";").shift();
        }
        return parts[1];
    }
    window.loadingSymbol=$(`<div id="loading">
        <span class="spinner-border spinner-border-sm" role="status" aria-hidden="true"></span>
        <span class="visually-hidden"></span>
    </div>`);
    $.fn.extend({
        removeDisableAttr:function(){$(this).prop("disabled",false);},
        addDisableAttr:function(){$(this).prop("disabled",true);},
        bootstrapHide:function(){$(this).addClass("d-none");},
        bootstrapShow:function(){$(this).removeClass("d-none");},
        showValidate:function(){$(this).addClass("is-invalid");},
        hideValidate:function(){$(this).removeClass("is-invalid");}
    });
    ["disable", "enable"].forEach(name => {
        window[name + "MultipleElements"] = elements => {
            for (let i = 0; 
                i < elements.length; 
                elements[i++][`${name === "disable" ? "add" : "remove"}DisableAttr`]()
            );
        }
    });
    window.redirect=url=>location.href=!url.startsWith("/")?url="/"+url:url;
    navigator.cookieEnabled || (function() {
        setTimeout(function() {
            console.warn("Cookies are disabled.");
            $("html").remove();
        }, 5);
    })();
})();
</script>
